              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>

          <MobileFormFactor>
            <FunctionFile resid="residFunctionFile"/>
            <ExtensionPoint xsi:type="MobileMessageReadCommandSurface">
              <Group id="mobMsgReadGroup">
                <Label resid="residLabel"/>
                <Control xsi:type="MobileButton" id="mobMsgRead">
                  <Label resid="residLabel"/>
                  <Icon>
                    <bt:Image size="25" scale="1" resid="icon-16"/>
                    <bt:Image size="25" scale="2" resid="icon-16"/>
                    <bt:Image size="25" scale="3" resid="icon-16"/>

                    <bt:Image size="32" scale="1" resid="icon-32"/>
                    <bt:Image size="32" scale="2" resid="icon-32"/>
                    <bt:Image size="32" scale="3" resid="icon-32"/>

                    <bt:Image size="48" scale="1" resid="icon-80"/>
                    <bt:Image size="48" scale="2" resid="icon-80"/>
                    <bt:Image size="48" scale="3" resid="icon-80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="residTaskPane"/>
                  </Action>
                </Control>
              </Group>
            </ExtensionPoint>
            <ExtensionPoint xsi:type="MobileOnlineMeetingCommandSurface">
              <Control xsi:type="MobileButton" id="insertMeetingButton">
                <Label resid="residMeetingLabel"/>
                <Icon>
                  <bt:Image size="25" scale="1" resid="mob-icon-24"/>
                  <bt:Image size="25" scale="2" resid="mob-icon-24"/>
                  <bt:Image size="25" scale="3" resid="mob-icon-24"/>

                  <bt:Image size="32" scale="1" resid="mob-icon-32"/>
                  <bt:Image size="32" scale="2" resid="mob-icon-32"/>
                  <bt:Image size="32" scale="3" resid="mob-icon-32"/>

                  <bt:Image size="48" scale="1" resid="mob-icon-48"/>
                  <bt:Image size="48" scale="2" resid="mob-icon-48"/>
                  <bt:Image size="48" scale="3" resid="mob-icon-48"/>
                </Icon>
                <Action xsi:type="ExecuteFunction">
                  <FunctionName>insertOnlineMeeting</FunctionName>
                </Action>
              </Control>
            </ExtensionPoint>
          </MobileFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Images>
          <bt:Image id="icon-16" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button16x16.png"/>
          <bt:Image id="icon-32" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button32x32.png"/>
          <bt:Image id="icon-80" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button80x80.png"/>
          <bt:Image id="mob-icon-24" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button32x32.png"/>
          <bt:Image id="mob-icon-32" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button32x32.png"/>
          <bt:Image id="mob-icon-48" DefaultValue="https://officedev.github.io/testing-assets/addins/images/button80x80.png"/>
        </bt:Images>
        <bt:Urls>
          <bt:Url id="residTaskPane" DefaultValue="https://officedev.github.io/testing-assets/addins/outlook/Home_olk.html"/>
          <bt:Url id="residFunctionFile" DefaultValue="https://officedev.github.io/testing-assets/addins/outlook/Home_olk.html"/>
        </bt:Urls>
        <bt:ShortStrings>
          <bt:String id="residLabel" DefaultValue="API Test"/>
          <bt:String id="residMeetingLabel" DefaultValue="Test meeting"/>
        </bt:ShortStrings>
        <bt:LongStrings>
          <bt:String id="residTooltip" DefaultValue="API Test"/>
        </bt:LongStrings>
